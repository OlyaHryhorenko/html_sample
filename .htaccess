RewriteEngine On
RewriteCond %{HTTP_HOST} ^www\.myssay\.com$ [NC]
RewriteRule ^(.*)$ https://myssay.com/$1 [R=301,L]
#
#
RewriteCond %{THE_REQUEST} ^[A-Z]{3,9}\ /([^/]+/)*(default|index|main)\.(html|php|htm)\ HTTP/ [NC]
RewriteRule ^(([^/]+/)*)(default|main|index)\.(html|php|htm)$ https://myssay.com/$1 [L,R=301]







